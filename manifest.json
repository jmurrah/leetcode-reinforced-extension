{
    "manifest_version": 2,
    "name": "LeetCode Repetition",
    "description": "",
    "version": "0.0.1",
    "permissions": [],
    "browser_specific_settings": {
      "gecko": {
        "id": "borderify@example.com"
      }
    },
    "content_scripts": [
        {
          "matches": ["https://leetcode.com/problems/*"],
          "js": ["./src/extension.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    }
  }